class Book {
  String title;
  String author;
  int numberOfPages;
  Book(this.title, this.author, this.numberOfPages);
}

void main() {
  var myBook = Book("Harry Potter and Sorcerer's stone", "J.K. Rowling", 309);
  print(
      "${myBook.title} by ${myBook.author} with ${myBook.numberOfPages} pages");
}
