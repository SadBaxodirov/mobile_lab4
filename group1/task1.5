
class Rectangle {
  double width;
  double height;
  Rectangle(this.width, this.height);
  double get area => width * height;
  double get perimeter => 2 * (width + height);
}

void main() {
  Rectangle myRectangle = Rectangle(20, 30);
  print("Area: ${myRectangle.area}");
  print("Perimeter: ${myRectangle.perimeter}");
}
