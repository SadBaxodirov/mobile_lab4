class Animal {
  void eat() {
    print("eating");
  }
}

class Lion extends Animal {
  @override
  void eat() {
    print("Eating meat");
  }
}

class Goat extends Animal {
  @override
  void eat() {
    print("Eating grass");
  }
}

void feed(Animal animal) {
  animal.eat();
}

void main() {
  Lion lion = Lion();
  Goat goat = Goat();
  feed(lion);
  feed(goat);
}
